precedence = (
    (‘right’ , ‘ASSIGN’),
    (‘right’ , ‘UPDATE’),
    (‘left’ ,  ‘NE’),
    (‘left’ ,  ‘LT’ , ‘LTE’ , ‘GT’ , ‘GTE’),
    (‘left’ , ‘PLUS’ , ‘MINUS’),
    (‘left’ , ‘TIMES’ , ‘DIVIDE’),
    (‘right’ , ‘ODD’),
    (‘left’ , ‘LPARENT’ , ‘RPARENT’),
)
 

